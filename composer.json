{
    "name": "sygic/monolog-module",
    "description": "Zend Framework 2 Module that provides functionality for Monolog library",
    "type": "library",
    "license": "proprietary",
    "repositories": [
        {
            "type": "composer",
            "url": "http://satis.ssd.sygic.local"
        }
    ],
    "require": {
        "php": ">=5.5",
        "monolog/monolog": "~1.9",
