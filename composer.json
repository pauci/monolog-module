{
    "name": "pauci/monolog-module",
    "description": "Zend Framework 2 Module that provides functionality for Monolog library",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=5.5",
        "monolog/monolog": "~1.9",
        "zendframework/zendframework": "~2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "predis/predis": "~1.0",
        "ruflin/elastica": "~1.4"
    },
    "autoload": {
        "psr-4": {
            "MonologModule\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
